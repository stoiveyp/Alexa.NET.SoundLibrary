<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <Version>1.0.0</Version>
    <Authors>Steven Pears</Authors>
    <Company />
    <NetStandardImplicitPackageVersion>1.6.0</NetStandardImplicitPackageVersion>
    <Description>Helper package to more quickly add the Alexa Sound Library to Alexa.NET SSML Responses</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/stoiveyp/Alexa.NET.SoundLibrary</PackageProjectUrl>
    <RepositoryUrl>https://github.com/stoiveyp/Alexa.NET.SoundLibrary</RepositoryUrl>
    <PackageTags>alexa, alexa.net, sound, ssml</PackageTags>
    <PackageReleaseNotes>Updated all the sounds to use the soundback:// protocol which helps minimize latency.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Alexa.NET" Version="1.4.2" />
  </ItemGroup>

</Project>
